(SELECT DISTINCT ship.shipment_no, line.order_no, cont.tracking_no, TO_CHAR(ship.actual_shipment_date,'MM/DD/YYYY HH24:MI:SS') actual_shipment_date, TO_CHAR(ship.expected_shipment_date,'MM/DD/YYYY HH24:MI:SS') expected_shipment_date, ship.pickticket_no, ship.scac, ship.carrier_service_code, ship.shipnode_key, stat.description status, line.release_no, line.prime_line_no, line.item_id, line.item_description, cont.container_scm, det.quantity, cont.container_gross_weight FROM sterling.yfs_shipment_line line, sterling.yfs_shipment ship, sterling.yfs_shipment_container cont, sterling.yfs_container_details det, sterling.yfs_status stat WHERE ship.shipment_key =line.shipment_key AND cont.shipment_key =ship.shipment_key AND det.shipment_container_key=cont.shipment_container_key AND det.shipment_line_key =line.shipment_line_key AND det.shipment_key =ship.shipment_key AND stat.status = ship.status AND process_type_key ='ORDER_DELIVERY' AND ship.document_type = '0001'