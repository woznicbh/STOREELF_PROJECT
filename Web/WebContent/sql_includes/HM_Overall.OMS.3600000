SELECT (CASE WHEN upper(trim(node.node_type)) = 'STORE' THEN 'STORE' ELSE DECODE (trim(rel.shipnode_key) , '873', 'EFC 1', '809', 'EFC 2', '819', 'EFC 3', '829', 'EFC 4', '865', 'RDC 865', '810', 'RDC 810', '840', 'RDC 840', '855', 'RDC 855', '830', 'RDC 830', '890', 'RDC 890', '885', 'RDC 885', '875', 'RDC 875', '860','RDC 860','869', 'LFC 869','879','LFC 879','859','LFC 859','889','LFC 889') END) AS "ship_node"	  , SUM(stat.status_quantity) AS "quantity"    , COUNT(DISTINCT head.order_no) AS "orders"   FROM  yfs_order_release rel     ,  yfs_order_release_status stat     ,  yfs_order_header head      ,  yfs_ship_node node WHERE trim(rel.shipnode_key) <> 'DSV'   AND rel.document_type ='0001' 	AND stat.order_release_key = rel.order_release_key    AND stat.status >='1500'    AND stat.status_quantity >0    AND head.order_header_key = rel.order_header_key    AND head.enterprise_key ='STOREELF.COM'    AND head.document_type ='0001' 	AND head.order_date > TO_CHAR(SYSDATE, 'DD-Mon-YY') 	AND head.order_date < TO_CHAR(SYSDATE+1, 'DD-Mon-YY')    AND rel.shipnode_key          = node.shipnode_key GROUP BY (CASE WHEN upper(trim(node.node_type)) = 'STORE' THEN 'STORE' ELSE DECODE (trim(rel.shipnode_key) , '873', 'EFC 1', '809', 'EFC 2', '819', 'EFC 3', '829', 'EFC 4', '865', 'RDC 865', '810', 'RDC 810', '840', 'RDC 840', '855', 'RDC 855', '830', 'RDC 830', '890', 'RDC 890', '885', 'RDC 885', '875', 'RDC 875', '860','RDC 860','869', 'LFC 869','879','LFC 879','859','LFC 859','889','LFC 889') END)