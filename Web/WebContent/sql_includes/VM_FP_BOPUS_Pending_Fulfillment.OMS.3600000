  SELECT   status_name AS DESCRIP,  sum(quantity) AS QTY,  status_no as STATUS  FROM  (select /*+ parallel(8) full(l) full(h) full(sl) full(s) full(st)*/  h.order_no,sl.shipment_key,st.status_name as status_name,sl.quantity as quantity, st.process_type_key, st.status as status_no from  sterling.yfs_order_line l,sterling.yfs_order_header h,sterling.yfs_shipment_line sl,sterling.yfs_shipment s,sterling.yfs_status st  where l.order_line_key > to_char(trunc(sysdate-2), 'YYYYMMDD')  and h.order_header_key = l.order_header_key  and l.fulfillment_type = 'STORE_PICKUP'  and sl.order_line_key = l.order_line_key  and s.shipment_key = sl.shipment_key  and s.status = st.status  and st.process_type_key = 'ORDER_DELIVERY'  and l.line_type != 'DSV'  and st.status > '1100.70.06.10'  and st.status < '1400' )GROUP BY status_name, status_no  ORDER BY to_number(replace(status_no, '.')) 