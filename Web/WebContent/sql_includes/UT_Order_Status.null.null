SELECT h.order_no "SALES_ORDER_NO", CASE WHEN min(s.STATUS) <= 2100 THEN 'Sourcing' WHEN min(s.status) >= 2100 and min(s.status) < 3700 THEN 'In Fulfillment' WHEN min(s.status) = 3700 THEN 'Shipped' WHEN min(s.status) = 9000 THEN 'Cancelled' ELSE 'Invoiced' END "Status" from sterling.yfs_order_header h,sterling.yfs_order_release_status s where h.order_no in (?) and h.DOCUMENT_TYPE = '0001' and h.ENTERPRISE_KEY = 'STOREELF.COM' and h.ORDER_HEADER_KEY = s.ORDER_HEADER_KEY and s.STATUS_QUANTITY > 0 and s.STATUS <> '1400' group by h.ORDER_NO