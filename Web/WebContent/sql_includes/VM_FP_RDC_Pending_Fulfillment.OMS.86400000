 select /*+ parallel(8) full(s) full(st) full(l) */  s.status,st.status_name as DESCRIP,st.process_type_key,sum(l.quantity) as QTY from  sterling.yfs_shipment s,sterling.yfs_status st,sterling.yfs_shipment_line l  where s.shipnode_key in (select shipnode_key from sterling.yfs_ship_node sn where node_type = 'RDC')   and s.shipment_key > to_char(sysdate-10,'YYYYMMDDHH24MISS')  and st.status = s.status  and l.shipment_key = s.shipment_key  and st.process_type_key = 'ORDER_DELIVERY'  and s.status < '1400'  and s.status in ('1300', '1100.025','1100.03','1100.04')  group by s.status, st.status_name, st.process_type_key order by s.status