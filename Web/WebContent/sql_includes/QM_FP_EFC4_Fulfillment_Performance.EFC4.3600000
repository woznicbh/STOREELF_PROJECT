WITH c1 AS ( SELECT ROUND(AVG(to_date(TO_CHAR(o.created_dttm,'YYYY-MON-DD-HH24:MI:SS'), 'YYYY-MON-DD-HH24:MI:SS') - to_date(TO_CHAR(o.order_date_dttm, 'YYYY-MON-DD-HH24:MI:SS'),'YYYY-MON-DD-HH24:MI:SS')),2) AS average FROM wmos_efc.outpt_orders o WHERE o.created_dttm > TRUNC(sysdate) ) , c2 AS (SELECT SUM(tax_id) backlog     FROM wmos_efc.orders i  WHERE i.do_status < 190  AND i.tc_order_id LIKE '0%') , c3 AS ( SELECT SUM(ptt.nbr_units) pickunitcnt FROM wmos_efc.prod_trkg_tran ptt WHERE ptt.MOD_DATE_TIME   >= TRUNC(sysdate) - 1 AND ptt.MOD_DATE_TIME <= TRUNC(sysdate) + 6/24 AND menu_optn_name IN ('Pack Ship Alone', 'Pick Gift Card','Pick to Carton','Pick to Tote','Trbl Pick to Carton', 'Pick to Carton WA') AND tran_type  ='500' AND tran_code IN ('001','007','010') ) , c4 AS ( SELECT COALESCE(SUM( ocd.size_value ),0) ttlshpunitcnt FROM wmos_efc.outpt_lpn_detail ocd WHERE ocd.created_dttm >= TRUNC(sysdate) + 6/24 ) , c5 AS 
 (SELECT COALESCE( SUM(opd.orig_order_qty - opd.shipped_qty),0) cnclunitcnt 
 FROM wmos_efc.outpt_order_line_item opd, wmos_efc.outpt_orders oph
 WHERE oph.tc_order_id = opd.tc_order_id AND opd.orig_order_qty - opd.shipped_qty > 0 
 AND oph.last_updated_dttm >= TRUNC(sysdate) + 6/24 AND opd.proc_stat_code = 90 and
 (oph.tc_order_id not like 'X%' and oph.tc_order_id not like 'C%' and oph.tc_order_id not like 'T%') ), c6 AS ( SELECT ROUND(c2.backlog/ ( SELECT COALESCE(ROUND(SUM(shipped_qty)/3),100000) FROM wmos_efc.outpt_order_line_item p WHERE TRUNC(p.last_updated_dttm)  >= TRUNC(sysdate-3) AND TRUNC(p.last_updated_dttm) < TRUNC(sysdate) ) ,2) expfulfill FROM c2 ) SELECT average, trim(TO_CHAR(backlog,'999,999,999,999,999'))      AS backlog, trim(TO_CHAR(pickunitcnt,'999,999,999,999,999'))  AS pickunitcnt, trim(TO_CHAR(ttlshpunitcnt,'999,999,999,999,999'))AS ttlshpunitcnt, trim(TO_CHAR(cnclunitcnt,'999,999,999,999,999'))  AS cnclunitcnt, expfulfill FROM c1, c2, c3, c4, c5, c6