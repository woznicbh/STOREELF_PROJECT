 select /*+ parallel(8) full(line) full(ors) full(head)*/  TRIM(TO_CHAR(sum(line.original_ordered_qty*line.unit_price),'$999,999,999,999,999.99')) "COUNT" from STERLING.yfs_order_release_status ors,  sterling.yfs_order_line line where ors.modifyprogid = 'SCHEDULE.0001' and ors.status = '9000' and not exists (select /*+ parallel(8)full(r)*/  'x' from sterling.yfs_order_release_status r where r.order_header_key = ors.order_header_key and r.order_line_key = ors.order_line_key and r.status = '1400') and line.order_line_key = ors.order_line_key and ors.order_release_status_key > TO_CHAR(SYSDATE ,'YYYYMMDD') 