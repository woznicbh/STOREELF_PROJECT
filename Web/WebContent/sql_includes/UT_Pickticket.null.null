select p.PKT_CTRL_NBR,p.CUST_PO_NBR,p.WHSE,p.ORD_TYPE,i.TOTAL_NBR_OF_UNITS,p.SHIPTO_STATE,p.ORD_DATE,p.CREATE_DATE_TIME,p.RTE_GUIDE_NBR, CASE i.STAT_CODE WHEN 10 THEN 'Unselected' WHEN 15 THEN 'Preview Wave Selected' WHEN 20 THEN 'Printed' WHEN 35 THEN 'In Packing'  WHEN 40 THEN 'Packed' WHEN 50 THEN 'Weighed' WHEN 60 THEN 'Manifested'  WHEN 90 THEN 'Shipped' WHEN 99 THEN 'Cancelled' ELSE 'Other' END "STAT_CODE"  from WMOS_EFC.PKT_HDR p,WMOS_EFC.PKT_HDR_INTRNL i where p.PKT_CTRL_NBR = i.PKT_CTRL_NBR and p.PKT_CTRL_NBR = '?'  UNION select p.PKT_CTRL_NBR,p.CUST_PO_NBR,p.WHSE,p.ORD_TYPE,i.TOTAL_NBR_OF_UNITS,p.SHIPTO_STATE,p.ORD_DATE,p.CREATE_DATE_TIME,p.RTE_GUIDE_NBR, CASE i.STAT_CODE WHEN 10 THEN 'Unselected' WHEN 15 THEN 'Preview Wave Selected' WHEN 20 THEN 'Printed' WHEN 35 THEN 'In Packing'  WHEN 40 THEN 'Packed' WHEN 50 THEN 'Weighed' WHEN 60 THEN 'Manifested'  WHEN 90 THEN 'Shipped' WHEN 99 THEN 'Cancelled'  ELSE 'Other' END "STAT_CODE"  from WMOS_EFC.PKT_HDR@EFC1 p,WMOS_EFC.PKT_HDR_INTRNL@EFC1 i where p.PKT_CTRL_NBR = i.PKT_CTRL_NBR and p.PKT_CTRL_NBR = '?'  UNION select p.PKT_CTRL_NBR,p.CUST_PO_NBR,p.WHSE,p.ORD_TYPE,i.TOTAL_NBR_OF_UNITS,p.SHIPTO_STATE,p.ORD_DATE,p.CREATE_DATE_TIME,p.RTE_GUIDE_NBR, CASE i.STAT_CODE WHEN 10 THEN 'Unselected' WHEN 15 THEN 'Preview Wave Selected' WHEN 20 THEN 'Printed' WHEN 35 THEN 'In Packing'  WHEN 40 THEN 'Packed' WHEN 50 THEN 'Weighed' WHEN 60 THEN 'Manifested'  WHEN 90 THEN 'Shipped' WHEN 99 THEN 'Cancelled'  ELSE 'Other' END "STAT_CODE"  from WMOS_EFC.PKT_HDR@EFC3 p,WMOS_EFC.PKT_HDR_INTRNL@EFC3 i where p.PKT_CTRL_NBR = i.PKT_CTRL_NBR and p.PKT_CTRL_NBR = '?'  UNION select p.tc_order_id,p.ext_purchase_order,p.o_facility_alias_id,p.order_type,to_number(NVL(p.tax_id, 0)),p.d_state_prov,p.order_date_dttm,  TO_DATE(TO_CHAR(p.created_dttm,'YYYY-MON-DD HH24:MI:SS'),'YYYY-MON-DD HH24:MI:SS'),p.lane_name,CASE p.DO_STATUS 	WHEN 110  THEN 'Unselected'  WHEN 115  THEN 'Preview Wave Selected'  WHEN 120  THEN 'Printed'  WHEN 140  THEN 'In Packing'  WHEN 150  THEN 'Packed'  WHEN 160  THEN 'Weighed'  WHEN 170  THEN 'Manifested'  WHEN 190  THEN 'Shipped'  WHEN 200  THEN 'Cancelled'  ELSE 'Other'  END STAT_CODE   FROM WMOS_EFC.ORDERS@EFC4 p  WHERE p.tc_order_id = '?' 